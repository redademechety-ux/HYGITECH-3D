  "private": true,
  "bugs": {
    "url": "https://github.com/redademechety-ux/hygitech-3d/issues"
  },
  "homepage": "https://hygitech-3d.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
